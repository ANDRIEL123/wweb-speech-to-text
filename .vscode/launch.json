{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "wweb-bot",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/wweb-bot/dist/index.js",
            "preLaunchTask": "tsc: build - wweb-bot/tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/wweb-bot/dist/**/*.js"
            ],
            "cwd": "${workspaceFolder}/wweb-bot"
        }
    ]
}