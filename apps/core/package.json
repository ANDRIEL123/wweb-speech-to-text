{
  "name": "@andriel123/core",
  "version": "1.0.0",
  "main": "src/index.js",
  "devDependencies": {
    "typescript": "^5.5.3",
    "@types/node": "^20.14.11"
  },
  "dependencies": {
    "@andriel123/queue": "workspace:*"
  },
  "scripts": {
    "build:queue": "pnpm --filter @andriel123/queue build",
    "build:core": "tsc",
    "build": "pnpm run build:queue && pnpm run build:core",
    "start": "node dist/index.js",
    "dev": "pnpm run build && node dist/index.js",
    "watch": "tsc --watch"
  }
}